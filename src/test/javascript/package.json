{
  "name": "livetrafik-sockjs-loadtest",
  "version": "1.0.0",
  "description": "SockJS STOMP load test for livetrafik WebSocket server",
  "type": "module",
  "main": "loadtest.js",
  "scripts": {
    "test": "node loadtest.js",
    "test:small": "node loadtest.js --clients=10 --duration=30s",
    "test:medium": "node loadtest.js --clients=50 --duration=3m",
    "test:large": "node loadtest.js --clients=200 --duration=5m"
  },
  "dependencies": {
    "@stomp/stompjs": "^7.0.0",
    "chalk": "^5.3.0",
    "commander": "^11.1.0",
    "sockjs-client": "^1.6.1",
    "ws": "^8.16.0"
  }
}
